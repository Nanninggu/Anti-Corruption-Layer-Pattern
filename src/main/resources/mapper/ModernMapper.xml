<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Design.Pattern.AntiCorruption.Layer.Pattern.ModernSystem.ModernMapper">

    <insert id="putDataFromModernTable" parameterType="Design.Pattern.AntiCorruption.Layer.Pattern.ModernSystem.ModernDto">
